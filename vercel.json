{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next",
      "config": {
        "output": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/next-server"
    }
  ],
  "env": {
    "NEXT_PUBLIC_VERCEL_ENV": "production"
  }
}
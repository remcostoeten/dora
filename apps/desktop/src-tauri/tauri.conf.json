{
    "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
    "productName": "Dora",
    "version": "0.0.9",
    "identifier": "com.Dora.dev",
    "build": {
        "frontendDist": "../dist",
        "devUrl": "http://localhost:1420",
        "beforeDevCommand": "bun run dev",
        "beforeBuildCommand": "bun run build",
        "removeUnusedCommands": true
    },
    "app": {
        "windows": [
            {
                "label": "main",
                "title": "Dora",
                "create": false,
                "width": 1280,
                "height": 900,
                "resizable": true,
                "fullscreen": false,
                "decorations": true,
                "transparent": true
            }
        ],
        "security": {
            "csp": null
        },
        "macOSPrivateApi": true
    },
    "bundle": {
        "active": true,
        "targets": "all",
        "icon": [
            "icons/32x32.png",
            "icons/128x128.png",
            "icons/128x128@2x.png",
            "icons/icon.icns",
            "icons/icon.ico"
        ]
    }
}
{
  "name": "@dora/data-generator",
  "version": "1.0.0",
  "description": "Interactive CLI tool for generating realistic dummy database tables with fake data",
  "main": "dist/index.js",
  "bin": {
    "data-gen": "./bin/data-gen"
  },
  "scripts": {
    "dev": "ts-node src/cli/index.ts",
    "build": "tsc",
    "start": "node dist/cli/index.js",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "cli",
    "faker",
    "data-generator",
    "dummy-data",
    "database",
    "sqlite",
    "postgresql",
    "mysql"
  ],
  "dependencies": {
    "@clack/prompts": "^0.7.0",
    "@faker-js/faker": "^8.4.0",
    "better-sqlite3": "^11.0.0",
    "chalk": "^5.3.0",
    "cli-table3": "^0.6.3",
    "commander": "^11.1.0",
    "csv-writer": "^1.6.0",
    "dotenv": "^16.3.1",
    "mysql2": "^3.6.5",
    "ora": "^7.0.1",
    "pg": "^8.11.3"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.8",
    "@types/node": "^20.10.6",
    "@types/pg": "^8.10.9",
    "@typescript-eslint/eslint-plugin": "^6.16.0",
    "@typescript-eslint/parser": "^6.16.0",
    "eslint": "^8.56.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
{
  "name": "dora",
  "version": "0.0.1",
  "private": true,
  "packageManager": "bun@1.1.0",
  "workspaces": [
    "apps/*"
  ],
  "scripts": {
    "dev": "turbo dev",
    "build": "turbo build",
    "lint": "turbo lint",
    "desktop:run": "bun --cwd apps/desktop run dev",
    "desktop:dev": "bun run --cwd apps/desktop tauri dev",
    "desktop:build": "bun --cwd apps/desktop run tauri build",
    "docs:dev": "bun --cwd apps/api-docs run dev",
    "docs:build": "bun --cwd apps/api-docs run build",
    "runner": "./dora-runner"
  },
  "devDependencies": {
    "turbo": "^2.3.0"
  },
  "dependencies": {
    "zustand": "^5.0.10"
  }
}